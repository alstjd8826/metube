extends ../layout/main
//- main/pug의 내용을 복사하여 쓰겠다.
//- block content 의 내용만 작성하면 된다.
include mixins/videoBlock

block content
    .videos
        h1 비디오 목록
        each item in videos
            +videoBlock({
                title: item.title,
                description: item.description,
                videoFile: item.videoFile
            })